<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_constraintTop_toBottomOf="@id/imageView3"
    app:layout_constraintBottom_toBottomOf="parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="match_parent"
            android:layout_height="150dp"
            android:src="@drawable/padtop"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageButton
            android:id="@+id/btn_kembali"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="18dp"
            android:layout_marginTop="48dp"
            android:background="@android:color/transparent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_baseline_arrow_back_24" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="45dp"
            android:fontFamily="@font/poppins_bold"
            android:text="Jadwal Praktek Dokter"
            android:textColor="@color/white"
            android:textSize="20dp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins_bold"
            android:text="Carilah Jadwal Dokter Yang Anda Tuju"
            android:textColor="@color/white"
            android:textSize="10dp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView3" />

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            app:layout_constraintTop_toBottomOf="@id/imageView3">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="15dp"
                android:layout_marginEnd="15dp"
                android:layout_marginTop="10dp"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/dokter"
                        android:layout_width="80dp"
                        android:layout_height="80dp"
                        app:srcCompat="@drawable/doctor_men" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:orientation="vertical"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="10dp">

                        <TextView
                            android:id="@+id/dktr"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Praktek: Senin-Kamis, 08:00-12:00"
                            android:textSize="15dp" />

                        <TextView
                            android:id="@+id/namedokter"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Nama Dokter :Afendi S.Ked"
                            android:textStyle="bold"
                            android:textAppearance="@style/TextAppearance.AppCompat.Headline"
                            android:textSize="20dp" />

                        <TextView
                            android:id="@+id/poli"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Poli: Umum"
                            android:textStyle="bold"
                            android:textSize="15dp" />

                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="10dp">

                    <ImageView
                        android:id="@+id/dokter2"
                        android:layout_width="80dp"
                        android:layout_height="80dp"
                        app:srcCompat="@drawable/doctor_girl" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:orientation="vertical"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="10dp">

                        <TextView
                            android:id="@+id/dktr2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Praktek: Senin-Kamis, 12:00-17:00"
                            android:textSize="15dp" />

                        <TextView
                            android:id="@+id/namedokter2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Nama Dokter :Putri S.Ked"
                            android:textStyle="bold"
                            android:textAppearance="@style/TextAppearance.AppCompat.Headline"
                            android:textSize="20dp" />

                        <TextView
                            android:id="@+id/poli2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Poli: Umum"
                            android:textStyle="bold"
                            android:textSize="15dp" />

                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="10dp">

                    <ImageView
                        android:id="@+id/dokter3"
                        android:layout_width="80dp"
                        android:layout_height="80dp"
                        app:srcCompat="@drawable/doctor_men" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:orientation="vertical"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="10dp">

                        <TextView
                            android:id="@+id/dktr3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Praktek: Jumat-Minggu, 08:00-12:00"
                            android:textSize="15dp" />

                        <TextView
                            android:id="@+id/namedokter3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Nama Dokter :Ahmad S.Ked"
                            android:textStyle="bold"
                            android:textAppearance="@style/TextAppearance.AppCompat.Headline"
                            android:textSize="20dp" />

                        <TextView
                            android:id="@+id/poli3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Poli: Umum"
                            android:textStyle="bold"
                            android:textSize="15dp" />

                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="10dp">

                    <ImageView
                        android:id="@+id/dokter4"
                        android:layout_width="80dp"
                        android:layout_height="80dp"
                        app:srcCompat="@drawable/doctor_girl" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:orientation="vertical"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="10dp">

                        <TextView
                            android:id="@+id/dktr4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Praktek: Senin-Kamis, 08:00-12:00"
                            android:textSize="15dp" />

                        <TextView
                            android:id="@+id/namedokter4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Nama Dokter :Sukma Sp.A"
                            android:textStyle="bold"
                            android:textAppearance="@style/TextAppearance.AppCompat.Headline"
                            android:textSize="20dp" />

                        <TextView
                            android:id="@+id/poli4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Poli: Anak"
                            android:textStyle="bold"
                            android:textSize="15dp" />

                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="10dp">

                    <ImageView
                        android:id="@+id/dokter5"
                        android:layout_width="80dp"
                        android:layout_height="80dp"
                        app:srcCompat="@drawable/doctor_men" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:orientation="vertical"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="10dp">

                        <TextView
                            android:id="@+id/dktr5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Praktek: Senin-Kamis, 12:00-17:00"
                            android:textSize="15dp" />

                        <TextView
                            android:id="@+id/namedokter5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Nama Dokter :Sholeh Sp.A"
                            android:textStyle="bold"
                            android:textAppearance="@style/TextAppearance.AppCompat.Headline"
                            android:textSize="20dp" />

                        <TextView
                            android:id="@+id/poli5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Poli: Anak"
                            android:textStyle="bold"
                            android:textSize="15dp" />

                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="10dp">

                    <ImageView
                        android:id="@+id/dokter6"
                        android:layout_width="80dp"
                        android:layout_height="80dp"
                        app:srcCompat="@drawable/doctor_girl" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:orientation="vertical"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="10dp">

                        <TextView
                            android:id="@+id/dktr6"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Praktek: Jumat-Minggu, 08:00-12:00"
                            android:textSize="15dp" />

                        <TextView
                            android:id="@+id/namedokter6"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Nama Dokter :Indah Sp.A"
                            android:textStyle="bold"
                            android:textAppearance="@style/TextAppearance.AppCompat.Headline"
                            android:textSize="20dp" />

                        <TextView
                            android:id="@+id/poli6"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Poli: Anak"
                            android:textStyle="bold"
                            android:textSize="15dp" />

                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>

        </androidx.cardview.widget.CardView>

    </androidx.constraintlayout.widget.ConstraintLayout>

</ScrollView>
